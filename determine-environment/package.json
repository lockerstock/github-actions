{
  "name": "determine-environment",
  "version": "0.0.0",
  "private": true,
  "description": "Determine Environment",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint . --ext .ts,.tsx",
    "package": "ncc build -m src/main.ts",
    "test": "mocha -r ts-node/register -t 120s 'tests/*.test.ts'",
    "all": "yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lockerstock/github-actions/determine-environment.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "octokit": "^2.0.7"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^14.0.0",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vercel/ncc": "^0.34.0",
    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-prettier": "^4.2.1",
    "js-yaml": "^4.1.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
